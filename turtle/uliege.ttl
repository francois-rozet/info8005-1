@prefix : <http://www.semanticweb.org/ontologies/2020/uliege#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix time: <http://www.w3.org/2006/time#> .
@base <http://www.semanticweb.org/ontologies/2020/uliege> .

<http://www.semanticweb.org/ontologies/2020/uliege> rdf:type owl:Ontology .

########################
# Classes & Properties #
########################

:includes rdf:type owl:ObjectProperty ,
                   owl:TransitiveProperty .

:includedIn rdf:type owl:ObjectProperty ;
            owl:inverseOf :includes .

# Agent

:Agent rdf:type owl:Class ;
       rdfs:subClassOf [ rdf:type owl:Restriction ;
                         owl:onProperty :name ;
                         owl:someValuesFrom xsd:string ] .

    :Person rdf:type owl:Class ;
            rdfs:subClassOf :Agent .

    :Organization rdf:type owl:Class ;
                  rdfs:subClassOf :Agent .

# Person

:Lecturer owl:equivalentClass [ rdf:type owl:Restriction ;
                                owl:onProperty :hosts ;
                                owl:someValuesFrom :Lecture ] ;
          rdfs:subClassOf :Person .

    :Professor rdf:type owl:Class ;
               rdfs:subClassOf :Lecturer ,
                               :Employee ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :teaches ;
                                 owl:someValuesFrom :Course ] .

:Student rdf:type owl:Class ;
         rdfs:subClassOf :Person ;
         owl:equivalentClass [ rdf:type owl:Restriction ;
                               owl:onProperty :registeredAt ;
                               owl:someValuesFrom :TeachingInstitution ] .

    :BachelorStudent owl:equivalentClass [ rdf:type owl:Restriction ;
                                           owl:onProperty :enrolledIn ;
                                           owl:someValuesFrom :BachelierProgram ] ;
                     rdfs:subClassOf :Student .

    :MasterStudent owl:equivalentClass [ rdf:type owl:Restriction ;
                                           owl:onProperty :enrolledIn ;
                                           owl:someValuesFrom :MasterProgram ] ;
                     rdfs:subClassOf :Student .

    :DoctoralStudent rdf:type owl:Class ;
                     rdfs:subClassOf :Student ,
                                     :Researcher .

:Employee rdf:type owl:Class ;
          rdfs:subClassOf :Person .

    :Dean owl:equivalentClass [ rdf:type owl:Restriction ;
                                owl:onProperty :deanOf ;
                                owl:someValuesFrom :Faculty ] ;
          rdfs:subClassOf :Employee .

    :Rector owl:equivalentClass [ rdf:type owl:Restriction ;
                                  owl:onProperty :rectorOf ;
                                  owl:someValuesFrom :University ] ;
            rdfs:subClassOf :Employee .

    :Researcher rdf:type owl:Class ;
                rdfs:subClassOf :Employee .

# Program

:Program rdf:type owl:Class ;
         rdfs:subClassOf [ rdf:type owl:Restriction ;
                           owl:onProperty :includes ;
                           owl:someValuesFrom :Course ] ;
         rdfs:subClassOf [ rdf:type owl:Restriction ;
                           owl:onProperty :taughtIn ;
                           owl:someValuesFrom :SuperiorInstitution ] .

    :BachelierProgram rdf:type owl:Class ;
                      rdfs:subClassOf :Program .

    :MasterProgram rdf:type owl:Class ;
                   rdfs:subClassOf :Program .

:enrolls rdf:type owl:ObjectProperty ;
         rdfs:domain :Program ;
         rdfs:range :Person .

:enrolledIn rdf:type owl:ObjectProperty ;
            owl:inverseOf :enrolls .

:Course rdf:type owl:Class ;
        rdfs:subClassOf [ rdf:type owl:Restriction ;
                          owl:onProperty :taughtBy ;
                          owl:someValuesFrom :Professor ] .
    
    :SuperiorCourse owl:unionOf ( :BachelierCourse :MasterCourse ) .

    :BachelierCourse owl:equivalentClass [ rdf:type owl:Class ;
                                           owl:intersectionOf ( :Course
                                                                [ rdf:type owl:Restriction ;
                                                                  owl:onProperty :includedIn ;
                                                                  owl:someValuesFrom :BachelierProgram ] ) ] .

    :MasterCourse owl:equivalentClass [ rdf:type owl:Class ;
                                        owl:intersectionOf ( :Course
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty :includedIn ;
                                                               owl:someValuesFrom :MasterProgram ] ) ] .

:teaches rdf:type owl:ObjectProperty ;
         rdfs:domain :Person .

:taughtBy rdf:type owl:ObjectProperty ;
          owl:inverseOf :teaches .

:taughtIn rdf:type owl:ObjectProperty ;
          rdfs:domain [ rdf:type owl:Class ; 
                        owl:unionOf (:Course :Program) ] ;
          rdfs:range :TeachingInstitution .

# Event

:Event rdf:type owl:Class ;
       rdfs:subClassOf [ rdf:type owl:Restriction ;
                         owl:onProperty time:hasTime ;
                         owl:someValuesFrom time:TemporalEntity ] .

    :Talk rdf:type owl:Class ;
          rdfs:subClassOf :Event .

        :Lecture rdf:type owl:Class ;
                 rdfs:subClassOf :Talk .

    :Conference rdf:type owl:Class ;
                rdfs:subClassOf :Event ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :includes ;
                                  owl:someValuesFrom :Talk ] .

:attends rdf:type owl:ObjectProperty ;
         rdfs:domain :Agent ;
         rdfs:range :Event .

:attendedBy rdf:type owl:ObjectProperty ;
            owl:inverseOf :attends .

:hosts rdf:type owl:ObjectProperty ;
       rdfs:domain :Agent ;
       rdfs:range :Event .

:hostedBy rdf:type owl:ObjectProperty ;
          owl:inverseOf :hosts .

# Publication

:Document rdf:type owl:Class .

:Publication rdf:type owl:Class ;
             rdfs:subClassOf :Document ;
             rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty :author ;
                               owl:someValuesFrom :Agent ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :title ;
                               owl:someValuesFrom xsd:string ] .

    :Book rdf:type owl:Class ;
          rdfs:subClassOf :Publication .

    :Article rdf:type owl:Class ;
             rdfs:subClassOf :Publication ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :doi ;
                               owl:cardinality "1"^^xsd:nonNegativeInteger ] .

        :BookArticle owl:equivalentClass [ rdf:type owl:Restriction ;
                                           owl:onProperty :publishedIn ;
                                           owl:someValuesFrom :Book ] ;
                     rdfs:subClassOf :Article .

        :ConferencePaper owl:equivalentClass [ rdf:type owl:Restriction ;
                                               owl:onProperty :publishedIn ;
                                               owl:someValuesFrom :Conference ] ;
                         rdfs:subClassOf :Article .

        :PeriodicalArticle owl:equivalentClass [ rdf:type owl:Restriction ;
                                                 owl:onProperty :publishedIn ;
                                                 owl:someValuesFrom :Periodical ] ;
                           rdfs:subClassOf :Article .

    :Periodical rdf:type owl:Class ;
                rdfs:subClassOf :Publication .

        :Journal rdf:type owl:Class ;
                 rdfs:subClassOf :Periodical .

        :Magazine rdf:type owl:Class ;
                  rdfs:subClassOf :Periodical .

    :Thesis rdf:type owl:Class ;
            rdfs:subClassOf :Publication .

        :DoctoralThesis rdf:type owl:Class ;
                        rdfs:subClassOf :Thesis .

        :MasterThesis rdf:type owl:Class ;
                      rdfs:subClassOf :Thesis .

:author rdf:type owl:ObjectProperty ;
        rdfs:domain :Agent .

:authorOf rdf:type owl:ObjectProperty ;
          owl:inverseOf :author .

:publishedIn rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :includedIn ;
             rdfs:domain :Article .

:title rdf:type owl:DatatypeProperty ;
       rdfs:range xsd:string .

:doi rdf:type owl:DatatypeProperty ;
     rdfs:range xsd:string .

# Organization

:TeachingInstitution rdf:type owl:Class ;
                     rdfs:subClassOf :Organization .

    :PrimaryInstitution rdf:type owl:Class ;
                        rdfs:subClassOf :TeachingInstitution .

    :SecondaryInstitution rdf:type owl:Class ;
                          rdfs:subClassOf :TeachingInstitution .

    :SuperiorInstitution rdf:type owl:Class ;
                         rdfs:subClassOf :TeachingInstitution .

        :HighSchool rdf:type owl:Class ;
                    rdfs:subClassOf :SuperiorInstitution .

        :University rdf:type owl:Class ;
                    rdfs:subClassOf :SuperiorInstitution ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty :includes ;
                                      owl:someValuesFrom :Campus ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty :hasAsRector ;
                                      owl:maxCardinality "1"^^xsd:nonNegativeInteger ] .

:hasAsRector rdf:type owl:ObjectProperty ;
             rdfs:domain :University ;
             rdfs:range :Person .

:rectorOf rdf:type owl:ObjectProperty ;
          owl:inverseOf :hasAsRector .

:Faculty rdf:type owl:Class ;
         rdfs:subClassOf :Organization ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :hasAsDean ;
                           owl:maxCardinality "1"^^xsd:nonNegativeInteger ] .

:hasAsDean rdf:type owl:ObjectProperty ;
           rdfs:domain :Faculty ;
           rdfs:range :Person .

:deanOf rdf:type owl:ObjectProperty ;
        owl:inverseOf :hasAsDean .

:Department rdf:type owl:Class ;
            rdfs:subClassOf :Organization ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :departementOfFaculty ;
                              owl:cardinality "1"^^xsd:nonNegativeInteger ] .

:departementOfFaculty rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf :includedIn ;
                     rdfs:domain :Department ;
                     rdfs:range :Faculty .

:facultyOfDepartement rdf:type owl:ObjectProperty ;
                     owl:inverseOf :departementOfFaculty .


:Diploma rdf:type owl:Class ;
         rdfs:subClassOf :Document .

:yieldsDiploma rdf:type owl:ObjectProperty ;
               rdfs:range :Diploma ;
               rdfs:domain :TeachingInstitution .

:diplomaFrom rdf:type owl:ObjectProperty ;
             owl:inverseOf :yieldsDiploma .

:PrimaryDiploma rdf:type owl:Class ;
                owl:intersectionOf ( :Diploma
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty :diplomaFrom ;
                                       owl:someValuesFrom :PrimaryInstitution ] ) .

:SecondaryDiploma rdf:type owl:Class ;
                  owl:intersectionOf ( :Diploma
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty :diplomaFrom ;
                                         owl:someValuesFrom :SecondaryInstitution ] ) .

:SuperiorDiploma rdf:type owl:Class ;
                 owl:intersectionOf ( :Diploma
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty :diplomaFrom ;
                                        owl:someValuesFrom :SuperiorInstitution ] ) .

:hasDiploma rdf:type owl:ObjectProperty ;
            rdfs:range :Diploma ;
            rdfs:domain :Person .

:hasAsPrerequisite rdf:type owl:ObjectProperty ;
                   rdfs:range :SuperiorCourse ;
                   rdfs:domain [ rdf:type owl:Class ;
                                 owl:unionOf (:Program :SuperiorCourse) ] .

:prerequisiteOf rdf:type owl:ObjectProperty ;
                owl:inverseOf :hasAsPrerequisite .

:registeredAt rdf:type owl:ObjectProperty ;
              rdfs:range :TeachingInstitution ;
              rdfs:domain :Student .

# Diploma restrictions

[ rdf:type owl:Restriction ;
  owl:onProperty :registeredAt ;
  owl:someValuesFrom :SuperiorInstitution] rdfs:subClassOf [ rdf:type owl:Restriction ;
                                                             owl:onProperty :hasDiploma ;
                                                             owl:someValuesFrom :SecondaryDiploma ] .
[ rdf:type owl:Restriction ;
  owl:onProperty :registeredAt ;
  owl:someValuesFrom :SecondaryInstitution] rdfs:subClassOf [ rdf:type owl:Restriction ;
                                                              owl:onProperty :hasDiploma ;
                                                              owl:someValuesFrom :PrimaryDiploma ] .

# Place

:Place rdf:type owl:Class .

    :Building rdf:type owl:Class ;
              rdfs:subClassOf :Place .

    :Campus rdf:type owl:Class ;
            rdfs:subClassOf :Place ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :campusOfBuilding ;
                              owl:minCardinality "1"^^xsd:nonNegativeInteger ] .

    :Room rdf:type owl:Class ;
          rdfs:subClassOf :Place ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :roomOfBuilding ;
                            owl:cardinality "1"^^xsd:nonNegativeInteger ] .

          :Amphitheatre rdf:type owl:Class ;
                        rdfs:subClassOf :Room .

          :Office rdf:type owl:Class ;
                  rdfs:subClassOf :Room .

:buildingfOfCampus rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf :includedIn ;
                   rdfs:domain :Building ;
                   rdfs:range :Campus .

:campusOfBuilding rdf:type owl:ObjectProperty ;
                  owl:inverseOf :buildingfOfCampus .

:roomOfBuilding rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :includedIn ;
                rdfs:domain :Room ;
                rdfs:range :Building .

:buildingOfRoom rdf:type owl:ObjectProperty ;
                owl:inverseOf :roomOfBuilding .
